def deco(func):
    def wrapper(*args, **kwargs):
        print("Вызов функции:", func.__name__)
        result = func(*args, **kwargs)
        list = []
        for i in result:
            list.append(i.upper())
        return list
    return wrapper
@deco
def create_to_list():
    s = "Привет"
    w = "Мир"
    return s, w
print(create_to_list())
