f = open("text.txt", "r", encoding = 'utf-8')
s = f.readlines()
f.close()
str1 = ""
for i in s:
    str1 += i
str1 = str1.lower()
d = {}
for i in str1:
    d[i] = d.get(i, 0) + 1
d = sorted(d.items(), key=lambda x: x[0])
print(d)
count = 0
len1 = 0
while True:
    if count > 10 or len1 == len(d):
        break
    for i in range(len(d)):
        for j in d[i]:
            print(j, end = " ")
        print()

        count += 1
        len1 += 1
